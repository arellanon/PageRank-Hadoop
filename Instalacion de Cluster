#Hadoop MapReduce
#Crea cluster:
#2
gcloud dataproc --region us-central1 clusters create cluster-arellanon-2 --subnet default --zone us-central1-a --master-machine-type n1-standard-2 --master-boot-disk-size 32 --num-workers 2 --worker-machine-type n1-standard-1 --worker-boot-disk-size 32 --project tallerlibre2-192622

